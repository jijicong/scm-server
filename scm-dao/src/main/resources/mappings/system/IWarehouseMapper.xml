<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.trc.mapper.system.IWarehouseMapper">
    <!--根据仓库的编码，查询仓库-->
    <select id="selectWarehouseNames" resultType="org.trc.domain.System.Warehouse">
        SELECT `code`,`name` from warehouse WHERE code IN
        <foreach collection="array" index="index" item="item" close=")" open="(" separator=",">
            #{item}
        </foreach>
    </select>

</mapper>